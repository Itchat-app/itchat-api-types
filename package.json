{
  "name": "@zaunapp/types",
  "version": "1.2.12",
  "description": "Zaun API types",
  "main": "src/lib.js",
  "types": "./src/lib.d.ts",
  "scripts": {
    "build": "node build.js",
    "format": "prettier src --write"
  },
  "private": false,
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zaunchat/zaun-api-types.git"
  },
  "bugs": {
    "url": "https://github.com/zaunchat/zaun-api-types/issues"
  },
  "devDependencies": {
    "@types/node": "^18.15.12",
    "node-fetch": "^3.3.1",
    "prettier": "^2.8.7"
  },
  "files": [
    "src/*"
  ],
  "exports": {
    ".": {
      "import": "./src/lib.js",
      "types": "./src/lib.d.ts"
    }
  },
  "prettier": {
    "printWidth": 120,
    "useTabs": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "none",
    "endOfLine": "lf",
    "semi": false
  },
  "publishConfig": {
    "access": "public"
  }
}
